                    margin: 2px;
                    text-align: center;
                    width: auto;

                    @media (max-width: @grid.mediaWidth()) {
                        margin-x: 0;
                    }

                    .big_img {
                        cursor: default;

                        &.fancybox {
                            position: relative;
                            display: block;
                            width: 100%;
                            height: 100%;
                            cursor: pointer;
                            z-index: 1;
                        }
                    }
                }

                .preview_img {
                    background-repeat: no-repeat;
                    background-size: cover;
                    background-position: center center;
                    overflow: hidden;

                    &:hover {
                        .overlay {
                            opacity: 1;
                        }
                    }
                }

                .img_double {
                    float: left;
                    width: 50%;

                    @media (max-width: @grid.mediaWidth()) {
                        width: 100%;
                    }

                    .big_img:after {
                        clear: both;
                        content: "";
                        display: block;
                    }

                    .overlay {
                        height: 566px;
                        width: 566px;

                        @media (max-width: @grid.mediaWidth()) {
                            height: 281px;
                        }

                        @media (max-width: @grid.mediaWidth(6)) {
                            width: 281px;
                        }
                    }
                }

                .img_side {
                    float: left;
                    width: 50%;

                    @media (max-width: @grid.mediaWidth()) {
                        width: 100%;
                    }

                    .img {
                        &:nth-child(1), &:nth-child(3) {
                            @media (max-width: @grid.mediaWidth()) {
                                margin-right: 4px;
                            }

                            @media (max-width: @grid.mediaWidth(6)) {
                                margin-right: 0;
                            }
                        }
                    }

                    .overlay {
                        height: 281px;
                    }
                }

                &:nth-child(even) {
                    .img_double {
                        float: right;
                    }
                }

                .img_h1 {
                    .preview_img {
                        height: 281px;
                    }
                }

                .img_h2 {
                    .preview_img {
                        height: 566px;

                        @media (max-width: @grid.mediaWidth()) {
                            height: 281px;
                        }
                    }
                }

                .img_w1 {
                    .preview_img {
                        width: 281px;
                    }
                }

                .img_w2 {
                    .preview_img {
                        width: 566px;

                        @media (max-width: @grid.mediaWidth(6)) {
                            width: 281px;
                        }
                    }
                }
            }

            &.no_opacity {
                .item_block {
                    .preview_img {
                        &:hover {
                            .overlay {
                                opacity: 0.3;
                            }
                        }
                    }
                }
            }
        }
    }

